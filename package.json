{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "prestart": "npm run migrate-dev:up",
    "start": "NODE_ENV=dev nodemon --exec ts-node src/server.ts",
    "poststart": "echo 'do after start'",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "pretest": "npm run migrate-test:up > /dev/null 2>&1",
    "test": "NODE_ENV=test jasmine-ts",
    "posttest": "npm run migrate-test:down > /dev/null 2>&1",
    "tsc": "tsc",
    "build": "tsc && cp database.js dist/",
    "prod": "npm run build && node dist/server.js",
    "migrate-dev:up": "NODE_ENV=dev db-migrate up --env dev",
    "migrate-dev:down": "NODE_ENV=dev db-migrate down --env dev",
    "migrate-test:up": "NODE_ENV=test db-migrate up --env test",
    "migrate-test:down": "NODE_ENV=test db-migrate down --count 9999 --env test",
    "migrate-dev:drop": "db-migrate db:drop dev --config ./database.js",
    "migrate-test:drop": "NODE_ENV=test db-migrate db:drop test --config ./database.js",
    "lint": "eslint './**/*.ts'",
    "lint:fix": "eslint './**/*.ts' --fix",
    "format": "prettier --write './**/*.ts'",
    "prepare": "husky install"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.10.13",
    "@types/node": "^20.6.3",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^5.2.2"
  }
}
